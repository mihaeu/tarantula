{
    "name": "mihaeu/tarantula",
    "description": "Another PHP crawler based on Guzzle.",
    "keywords": ["crawler", "spider"],
    "homepage": "https://github.com/mihaeu/tarantula",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Haeuslmann",
            "email": "haeuslmann@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mihaeu\\Tarantula\\": ["src/", "tests/unit/"]
        }
    },
    "require": {
        "guzzle/guzzle": "3.*",
        "symfony/console": "2.7.*",
        "symfony/process": "2.7.*",
        "symfony/filesystem": "2.7.*",
        "symfony/dom-crawler": "2.7.*",
        "symfony/css-selector": "2.7.*",
        "zaininnari/html-minifier": "0.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "satooshi/php-coveralls": "dev-master",
        "johnkary/phpunit-speedtrap": "dev-master"
    },
    "bin": [
        "bin/tarantula"
    ]
}
