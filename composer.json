{
    "name": "mihaeu/tarantula",
    "description": "Another PHP crawler based on Guzzle.",
    "keywords": ["crawler", "spider"],
    "homepage": "https://github.com/mihaeu/tarantula",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Haeuslmann",
            "email": "haeuslmann@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Mihaeu": "src"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "guzzle/guzzle": "3.*",
        "symfony/dom-crawler": "2.*",
        "symfony/console": "2.*",
        "zaininnari/html-minifier": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "satooshi/php-coveralls": "~0.6"
    },
    "bin": [
        "bin/tarantula"
    ]
}
